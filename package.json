{
  "name": "maqraaty_2-server",
  "productName": "مقرأتي",
  "version": "1.0.0",
  "description": "مقرأتي",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon app.js",
    "electron": "electron-forge start",
    "winpackage": "electron-packager . مقرأتي --platform=win32 --arch=x64 --overwrite --app-version=1.0.0 --build-version=1.0.0 --win32metadata.ProductName=مقرأتي --FileDescription=مقرأتي --appname=مقرأتي --win32metadata.CompanyName=ng --app-copyright=ng --icon=dist/maqraaty_2/browser/favicon.ico",
    "package": "electron-forge package",
    "make": "electron-forge make --platform=win32 --arch=x64",
    "publish": "electron-forge publish",
    "pkg": "electron-packager . مقرأتي --platform=win32 --arch=x64",
    "build-installer": "electron-builder"
  },
  "build": {
    "appId": "maqraaty_2",
    "files": [
      "./dist/**/*",
      "./routes/*",
      "./views/*",
      "./models/*",
      "./controller/*",
      "./main.js",
      "./uploads/*",
      "./app.js",
      "./helper/*",
      "./middleware/*",
      "./static/*",
      "./utils/*",
      "./preload.js"
    ],
    "directories": {
      "output": "${platform}/interface",
      "buildResources": "dist"
    },
    "extraFiles": [
      {
        "from": "uploads",
        "to": "uploads",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "quran.png",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "favicon.ico",
      "installerHeaderIcon": "favicon.ico",
      "uninstallerIcon": "favicon.ico",
      "uninstallDisplayName": "مقرأتي",
      "shortcutName": "مقرأتي",
      "license": "license.txt",
      "allowToChangeInstallationDirectory": false
    }
  },
  "config": {
    "forge": {
      "electronPackagerConfig": {
        "icon": "/dist/maqraaty_2/browser/favicon.ico"
      },
      "packagerConfig": {
        "icon": "/dist/maqraaty_2/browser/favicon.ico"
      },
      "publishers": [
        {
          "name": "@electron-forge/publisher-s3",
          "platforms": [
            "darwin",
            "linux",
            "win"
          ],
          "config": {
            "bucket": "my-bucket",
            "folder": "my/key/prefix"
          }
        },
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Ahmed21kh",
              "name": "electron"
            },
            "prerelease": false,
            "draft": true
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_quick_start",
            "options": {
              "icon": "/dist/maqraaty_2/browser/favicon.ico"
            }
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "config": {
            "name": "electron_quick_start",
            "options": {
              "icon": "/dist/maqraaty_2/browser/favicon.ico"
            }
          },
          "platforms": [
            "darwin",
            "win"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "icon": "/dist/maqraaty_2/browser/favicon.ico"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "keywords": [],
  "author": "Ahmed Khaled",
  "license": "ISC",
  "dependencies": {
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-wix": "^7.4.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "connect": "^3.7.0",
    "connect-livereload": "^0.6.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.9",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-wix-msi": "^5.1.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "formidable": "^3.5.1",
    "livereload": "^0.9.3",
    "maqraaty_2-server": "file:",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "mongoose-sequence": "^6.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "xlsx": "^0.18.5",
    "xlsx-mongo": "^1.0.15"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@electron-forge/publisher-s3": "^7.4.0",
    "@electron/packager": "^18.3.3",
    "electron": "^30.0.1",
    "electron-builder": "^25.1.8",
    "electron-reloader": "^1.2.3"
  }
}
